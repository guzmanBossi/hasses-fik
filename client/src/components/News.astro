---
import { Icon } from "astro-icon";
import OnormalUtanRiktning from "../assets/OnormalUtanRiktning-top.jpg";
import { Image } from "@astrojs/image/components";
---

<div class="flex justify-center mx-auto bg-lime-950cd">
    <div class="w-full mx-auto max-w-screen-md">
        <div>
            <Image
                src={OnormalUtanRiktning}
                alt="bakgrundsbild"
                class="w-auto"/>
            <div>
                <div class="h-screen mx-auto px-4 py-4 max-w-screen-md bg-gradient-to-b from-green-950 to-green-900">
                    <h2 class="text-4xl font-semibold tracking-wide text-center text-yellow-700 sm:text-5xl">
                        Kalender
                    </h2>
                </div>
            </div>
        </div>
    </div>
</div>
